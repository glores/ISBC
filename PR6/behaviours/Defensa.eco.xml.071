        <beh:aggregation aggregator="AND">
          <condition comparator="&gt;=">
            <left type="sensor" value="fieldBallX" />
            <right type="constant" value="0" />
          </condition>
          <condition comparator="&gt;=">
            <left type="sensor" value="getBallR" />
